    "main": "index.js",
    "dependencies": {},
    "devDependencies": {
        "@types/jasmine": "^4.3.1",
        "jasmine": "^4.6.0",
        "nodemon": "^3.1.4",
        "nyc": "^15.1.0",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "prebuild": "rimraf ./index.js ./index.js.map",
        "build": "npm run tsc",
        "tsc": "tsc",
        "test:spec": "NODE_ENV=test ts-node node_modules/.bin/jasmine 'src/**/*.spec.ts'",
        "test": "nyc npm run test:spec",
        "test:watch": "npm run watch -- 'npm run test'",
        "tdd": "npm run watch -- 'npm run test:spec'",
        "watch": "nodemon -w src -w spec -e ts,js -x"
    }
